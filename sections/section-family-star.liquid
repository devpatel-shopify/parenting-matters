                                  <div class="range-slider__value range-slider__value--min"></div>
                                  <div class="range-slider__value range-slider__value--max"></div>
                                </div>
                                {% comment %} <input class="range-slider__slider range-slider__slider--from" type="range" value="5" min="1" max="12" /> {% endcomment %}
                                <input  data-que="{{ title | handleize }}" class="range-slider__slider range-slider__slider--to range_{{ title | handleize }}" type="range" value="1" min="1" max="10" />
                                <div class="range-slider__scale">
                                  {%- for i in (1..10) -%}
                                    <i>{{ i }}</i>
                                  {%- endfor -%}
                                </div>
                              </div>

                              {% comment %} <div class="range_inner">
                                <input class="sliderinput" id="slider{{ question | handleize }}" data-input-index="{{ forloop.index }}" type="range" name="{{ bs.klaviyo_id | handleize }}" min="1" max="10" value="1" data-val="1">
                                <input type="hidden" id="slidervalue{{ question | handleize }}" value="1" >
                                <div class="range_steps">
                                  {%- for i in (1..10) -%}
                                    <span class="step">&nbsp;</span>
                                  {%- endfor -%}
                                </div>
                              </div> {% endcomment %}

                            </div>
                            <div class="btn-flex">
                              <button type="button" class="button button_back js-back-btn">{% render 'icon-left-arrow' %}Back</button>
                              <button type="button" class="button button_next js-next-btn" data-type="inner-slide-range">Next{% render 'icon-arrow' %}</button>
                            </div>
                          </div>
                        {%- endif -%}
                      {%- endcapture -%}
                      
                      {{ metaobjectHandle | json }} : {{ metaobjectContent | strip_newlines | json }}{%- unless totalData == tmp -%},{%- endunless -%}

                    {%- endif -%}
                  {%- endfor -%}
                  {%- if block.settings.metaobject_steps != blank -%}
                  {%- endif -%}
                {%- endcapture -%}

                <script type="application/json" data-metaobjects>
                  {
                    {{ metaCotent }}
                  }
                </script>
              </div>

            {% when 'dropdown' %}
              <div class="slide " data-klaviyoid="{{ bs.klaviyo_id }}">
                {% if bs.title != blank %}
                  <h2 class="slide-title {{ heading_font }}">({{ forloop.index }}) - {{ bs.title | newline_to_br }}</h2>
