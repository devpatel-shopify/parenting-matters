                  {% assign fields = fields | append: inputName | append: "," %}


                  <input type="hidden" name="{{ bs.title | handleize }}_{{ i }}_values" id="{{ bs.title | handleize }}_{{ i }}_values" value >
                  {% assign inputName = bs.title | handleize | append: '_' | append: i | append: '_values'    %}
                  {% assign fields = fields | append: inputName | append: "," %}

                  <input type="hidden" name="{{ bs.title | handleize }}_{{ i }}_score" id="{{ bs.title | handleize }}_{{ i }}_score" value >
                  {% assign inputName = bs.title | handleize | append: '_' | append: i | append: '_score'   %}
                  {% assign fields = fields | append: inputName | append: "," %}

                {%- endfor -%}

                <div class="btn-flex">
