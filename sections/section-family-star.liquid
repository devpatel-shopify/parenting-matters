                        type="checkbox"
                        id="{{ option | handleize }}-{{forloop.index}}"
                        data-input-index="{{ forloop.index }}"
                        name="{{ bs.klaviyo_id | handleize }}"
                        value="{{ option | strip_newlines | escape }}"
                        data-val="{{ option | strip_newlines | escape }}"
                        data-text-handleize = "{{ option | handleize }}"
                        data-que="{{ bs.title | handleize }}"
                        required
                      >
                      <label for="{{ option | handleize }}-{{forloop.index}}">{{ option }}</label>
                    </div>
                  {%- endfor -%}
                </div>
